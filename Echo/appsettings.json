{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Console": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Localization": {
    "SupportedCultures": [ "en", "ru" ],
    "SupportedUICultures": [ "en", "ru" ],
    "DefaultRequestCulture": {
      "Culture": "en",
      "UICulture": "en"
    }
  },
  "AllowedHosts": "*"
}
